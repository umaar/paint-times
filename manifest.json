{
  "name": "Paint Times",
  "description": "Shows paint times for a page",
  "version": "0.1",
  "permissions": [
    "tabs",
    "debugger"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Paint Times"
  },
  "manifest_version": 2
}

